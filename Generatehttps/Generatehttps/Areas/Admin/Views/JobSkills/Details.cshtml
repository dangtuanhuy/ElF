@model Generatehttps.Models.JobSkill

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h2 class="text-info">JobSkill Details</h2>
<br />
<div class=" border  backgroundWhite">
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Job.Name"></label>
        </div>
        <div class="col-5">
            <input asp-for="Job.Name" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Skill.Name"></label>
        </div>
        <div class="col-5">
            <input asp-for="Skill.Name" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-5 offset-2">
            <partial name="_DetailsAndBackToListButton" model="Model.Id" />
        </div>
    </div>

</div>
