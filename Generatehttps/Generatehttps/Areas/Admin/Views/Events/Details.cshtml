@model Generatehttps.Models.Event
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h2 class="text-info">Event Details</h2>
<br />
<div class=" border  backgroundWhite">
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="TiTle"></label>
        </div>
        <div class="col-5">
            <input asp-for="TiTle" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Address"></label>
        </div>
        <div class="col-5">
            <input asp-for="Address" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Date"></label>
        </div>
        <div class="col-5">
            <input asp-for="Date" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Hour"></label>
        </div>
        <div class="col-5">
            <input asp-for="Hour" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Information"></label>
        </div>
        <div class="col-5">
            <input asp-for="Information" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="TiTle"></label>
        </div>
        <div class="col-5">
            <input asp-for="TiTle" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-5 offset-2">
            <partial name="_DetailsAndBackToListButton" model="Model.Id" />
        </div>
    </div>

</div>

